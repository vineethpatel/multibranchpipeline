            steps { 
               sh 'echo "This is master branch"' 
            }
        }
     
        stage('sprint1') { 
            steps { 
               sh 'echo "sprint1 branch..."'
            }
        }

        stage("sprint2") { 
             steps { 
                sh 'echo "sprint2 branch..."'
            }
        }  
    }
}
